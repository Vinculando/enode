<div class="form-login-registration">

  <%= form_for(resource, as: resource_name, url: registration_path(resource_name)) do |f| %>
  <%= devise_error_messages! %>

  <h2 class="form-login-heading">Registro Estudiante</h2>
  <div class="login-wrap">

    <div class="field">
      <%= f.label :email %><br/>
      <%= f.email_field :email, autofocus: true, class: "form-control" %>
    </div>

    <div class="field">
      <%= f.label :rut %><br />
      <%= f.text_field :rut, class: "form-control" %>
    </div>

    <div class="field">
      <%= f.label :nombre %><br />
      <%= f.text_field :nombre, class: "form-control" %>
    </div>

    <div class="field">
      <%= f.label 'Apellido Paterno' %><br />
      <%= f.text_field :paterno, class: "form-control" %>
    </div>

    <div class="field">
      <%= f.label 'Apellido Materno' %><br />
      <%= f.text_field :materno, class: "form-control" %>
    </div>

    <div class="field">
      <%= f.label 'Fecha nacimiento' %><br />
      <%= f.text_field :fechanac, {class: "form-control", 'data-provide' => 'datepicker'} %>
    </div>

    <div class="field">
      <%= f.label 'Genero' %><br />
      <%= f.select(:genero, [ 'Masculino', 'Femenino' ],{include_blank: 'Selecciona genero'}, class: "form-control")%>
    </div>

    <div class="field">
      <%= f.label 'ciudad' %><br />
      <%= grouped_collection_select :user, :city_id, Region.order(:nombre), :cities, :nombre, :id, :nombre, {prompt: true}, {class: "form-control selectpicker", "data-live-search" => "true"}  %>
    </div>

    <div class="field">
      <%= f.label :password %>
      <% if @minimum_password_length %>
      <em>(<%= @minimum_password_length %>
        caracteres minimo)</em>
      <% end %><br/>
      <%= f.password_field :password, autocomplete: "off", class: "form-control" %>
    </div>

    <div class="field">
      <%= f.label :password_confirmation %><br/>
      <%= f.password_field :password_confirmation, autocomplete: "off", class: "form-control" %>
    </div>

    <div class="actions">
    </br>
      <%= f.submit "Sign up", class: "btn btn-theme btn-block" %>
    </div>
    <% end %>
    <div class="registration">
    </br>
        Tienes una cuenta?<br/>
    <%= render "users/shared/links" %>
  </div>


  </div>
</div>
